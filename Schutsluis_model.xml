const int MIN_INVAARTIJD = 12;
const int EXTRA_INVAARTIJD = 6;

int vaarTijd =0;

void invaren()
{
    pool[side]--;
    kolk++;
}

void uitvaren()
{
    kolk--;
}

void calculateTime()
{
    int aantalBoten = pool[side]&gt;=KOLK_SIZE ? KOLK_SIZE : pool[side];

    vaarTijd = (MIN_INVAARTIJD + (aantalBoten * EXTRA_INVAARTIJD)) / aantalBoten;
}

